<div class="place-card">
  <div class="place-card-image" style="background-image: url('<%= place.photo %>')">
  </div>
  <div class="place-card-content pt-2">
    <div class="place-card-title d-flex align-items-center mb-0 pb-0">
      <h4><%= place.name.upcase %></h4>
      <span class="point px-1 pb-2"> · </span>
      <h4>PARIS 11</h4>
    </div>
    <p class="mb-0"><%= place.name %></p>
    <div class="place-card-votes d-flex justify-content-start pt-1">
      <%= render 'shared/stars' %>
      <div class="votes pl-1 d-flex justify-content-start">
        <p>110</p>
        <span class="point px-1 pb-2"> · </span>
        <p>SuperApéro</p>
      </div>
    </div>
  </div>
</div>




